#!/bin/csh -f


diff $argv[1] $argv[2] > /dev/null 


if ($? == 0) then
#    echo  -e "no differenrce \n do nothing \n"
    echo   "no differenrce  do nothing "
else 


git add -A
git commit -am "add $argv[1] and $argv[2]"

cp $argv[1] $argv[2]

git add .
git commit -am "show me the diff"


git push


endif
